<view class="view"><view class="caigou"><view class="input"><input placeholder="搜索" data-event-opts="{{[['input',[['__set_model',['','value','$event',[]]]]]]}}" value="{{value}}" bindinput="__e"/><view class="grace-iconfont icon-search"></view></view><view class="list"><block wx:for="{{dianData}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view class="item" data-item="{{index}}" data-event-opts="{{[['tap',[['toShangPin',['$event']]]]]}}" bindtap="__e"><view class="title"><text class="name">{{item.name}}</text><text class="guanzu" style="font-size:0.5rem;color:#888888;margin-left:20rpx;">{{item.guanzu+"人关注"}}</text></view><view class="img"><swiper class="swiper" indicator-dots="false" autoplay="true" interval="{{item.interval}}" duration="1000" circular="true"><swiper-item><image src="../../static/img/banner1.jpg"></image></swiper-item><swiper-item><image src="../../static/img/banner2.jpg"></image></swiper-item></swiper></view><view class="buttonview"><button type="primary" data-index="{{index}}" data-event-opts="{{[['tap',[['jiameng',['$event']]]]]}}" catchtap="__e">加盟合作</button><button type="warn" data-event-opts="{{[['tap',[['guanzu']]]]}}" catchtap="__e">关注</button></view><view hidden="{{!(item.jiameng)}}" class="jiameng"><view>{{"电话:"+item.phone}}</view><view>{{"地址:"+item.place}}</view></view></view></block></view></view></view>